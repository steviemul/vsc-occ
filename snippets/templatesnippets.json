{
    "CCi18n Binding": {
        "prefix":"i18n",
        "body": [
            "<span data-bind=\"widgetLocaleText: {value:'${1:resourceKey}', attr:'${2:altText}'\"></span>"
        ]
    },
    "OCC Widget": {
        "prefix":"widget",
        "body":[
            "<!-- ko if: loaded -->",
            "  <div class=\"templateRegion\" id=\"${1:my-widget}\" data-oc-id=\"${2:my-widget}\">",
            "  </div>",
            "<!-- /ko -->"
        ]
    },
    "KO With": {
        "prefix":"with",
        "body": [
            "<!-- ko with: ${1:item} -->",
            "  <span></span>",
            "<!-- /ko -->"
        ]
    }
}